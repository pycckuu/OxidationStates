<h1>Listing oxidation_states</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @oxidation_states.each do |oxidation_state| %>
      <tr>
        <td><%= link_to 'Show', oxidation_state %></td>
        <td><%= link_to 'Edit', edit_oxidation_state_path(oxidation_state) %></td>
        <td><%= link_to 'Destroy', oxidation_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

